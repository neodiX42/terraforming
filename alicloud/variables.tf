variable "host_password" {
  default = ""
}

variable "access_key" {
  default = ""
}
variable "secret_key" {
  default = ""
}

variable "region" {
  default = "eu-central-1"
}
variable "zoneid" {
  default = "eu-central-1a"
}
variable "instance_type" {
  default = "ecs.c6.6xlarge"
}
variable "instances_number" {
  default = 256
